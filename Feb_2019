create Function [SourceData].[GetAddressLine](@AddressNumber Int)
Returns varchar(100)
AS
Begin
	Declare @str varchar(100)
	
	select @str=LEFT(COALESCE(@str+',','')+ ad.TEXT,100)
	from SourceData.ADDRLINE ad where ad.ADDRNBR = @AddressNumber
	order by ad.LINENBR
	Return @str



END
