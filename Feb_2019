CREATE TABLE [SourceData].[CUP003](
	[CUBK] [numeric](3, 0) NULL,
	[CUNBR] [nvarchar](10) NULL,
	[CUSTAT] [nvarchar](1) NULL,
	[CUALT] [nvarchar](1) NULL,
